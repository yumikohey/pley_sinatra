<% @lowscore_biz.each do |business| %>
  <% biz_id = business.business_id %>
  <% biz = Business.where(business_id: biz_id)[0] %>
	<div class="row">
		<div class="col-md-2">
			<%= erb :view_360, :locals => { :biz => biz } %>
		</div>

		<div class="col-md-7 col-md-offset-2">
		   <h2><a href="/<%= biz.business_id%>"><%= biz.name.titleize %></a></h2>
		   <%= erb :biz_vio_desc, :locals => { :biz_id => biz_id } %>
		</div>
	</div>
<% end %>